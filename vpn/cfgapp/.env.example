# CFG App Configuration Example
# Copy this file to .env and customize as needed

# API Configuration
CONFIG_HOST=your-config-host.com
API_HOST=your-api-host.com

# Authentication
SALT=your-secret-salt

# IP Aggregation Settings
# Controls how IP networks are aggregated
IPV4_BLOCK_PREFIX=18
IPV6_BLOCK_PREFIX=32

# Network Compaction Settings
# Intelligent network compaction is ENABLED by default
# Reduces large CIDR lists while maintaining 100% coverage
# For AWS IP lists: 1633 networks â†’ ~200 networks (87.8% reduction, 3x coverage)
ENABLE_COMPACTION=true
COMPACT_TARGET_MAX=200
COMPACT_MIN_PREFIX_V4=11
COMPACT_MIN_PREFIX_V6=32

# To disable compaction (not recommended for large lists):
# ENABLE_COMPACTION=false

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Proxy Configuration
PROXY_CONFIG=path/to/config.json
OBFS_PASSWORD=
HYSTERIA2_PORT=47012
HYSTERIA2_V2_PORT=47013
VLESS_PORT=8443
HTTPS_PORT=443
REALITY_PRIVATE_KEY=
REALITY_PUBLIC_KEY=
REALITY_SHORT_ID=c047f3e99c90ff71
BASE_URL=

# Logging
LOG_LEVEL=INFO
